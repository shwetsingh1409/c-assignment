//C program to swap two numbers USING 3RD VARIABLE AND WITHOUT USING 3RD VARIABLE.

#include<stdio.h> 
int main() { 
int x, y, t; 
printf("Enter the value of x: "); 
scanf("%d", &x); 
printf("Enter the value of y: "); 
scanf("%d",&y); 
t =x; 
x = y;
y = t;
 printf("After swapping x=%d, b=%d", x, y); 
return 0;
 }
 // without using third variable
 #include<stdio.h>

int main(){
int a,b;
printf("Enter value of A: ");
scanf("%d", & a);
printf("Enter value of B: ");
scanf("%d", & b);

a=a^b;
b=b^a;
a=a^b;
printf("After swaping \n a=%d\n b=%d\n",a,b);
    return 0;
}
