//C program to print all Perfect numbers between 1 to n AND Check a given number is Perfect numbers or not.

#include <stdio.h>
int main(){
	int n;
	scanf("%d",&n);
	for(int i=1;i<=n;i++)
	{
		if(i%9==1)
		{
			printf("%d\n",i);
		}
	}
	if(n%9==1)
	{
		printf("the given number is perfect");
	}
	else
	printf("\nthe given given is not perfect");
	return 0;
}
