//C program to print all natural numbers AND sum of it from 1 To n
#include <stdio.h>
int main() {
    int n, i, sum = 0;

    printf("Enter a positive integer: ");
    scanf("%d", &n);
    printf("natural number from 1 to %d is\n",n);
    for (i = 1; i <= n; ++i) {
    	printf("%d\n",i);
        sum = sum+i;
    }

    printf("Sum = %d", sum);
    return 0;
}
